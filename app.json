        },
        "tokens": [
          {
            "name": "capability",
            "type": "string",
            "title": {
              "en": "Capability",
              "de": "Attribut",
              "nl": "Capability"
            }
          },
          {
            "name": "value_boolean",
            "type": "boolean",
            "title": {
              "en": "Value",
              "de": "Wert",
              "nl": "Waarde"
