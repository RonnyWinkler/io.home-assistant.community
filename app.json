            "placeholder": {
              "en": "Select the service",
              "de": "Wähle den Dienst",
              "nl": "Selecteer de dienst"
            }
          },
          {
            "type": "autocomplete",
            "name": "entity",
            "required": false,
            "title": {
              "en": "Entity",
              "de": "Entität",
              "nl": "Entiteit"
