            "placeholder": {
              "en": "{\"parameter\": \"value\"}",
              "de": "{\"Parameter\": \"Wert\"}",
              "nl": "{\"parameter\": \"waarde\"}"
            }
          }
        ],
        "id": "callServiceEntity"
      },
      {
        "highlight": true,
        "title": {
          "en": "Select sound mode.",
          "de": "Wähle Klangmodus.",
          "nl": "Selecteer geluidsmodus"
